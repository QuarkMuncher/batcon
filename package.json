{
  "name": "batcon",
  "description": "batch image conversion tool",
  "main": "main.js",
  "version": "0.0.40",
  "author": "QuarkMuncher",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuarkMuncher/batcon.git"
  },
  "scripts": {
    "start": "electronmon .",
    "pack": "electron-builder --dir",
    "build": "electron-builder -w --publish never",
    "deploy": "electron-builder -w --publish always"
  },
  "electronmon": {
    "patterns": [
      "!test-images/**"
    ]
  },
  "devDependencies": {
    "electron": "^8.2.3",
    "electron-builder": "^22.5.1",
    "electronmon": "^2.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "electron-updater": "^4.3.1",
    "prettier": "^2.0.5",
    "sharp": "^0.29.0"
  }
}
